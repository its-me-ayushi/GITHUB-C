#include <iostream>

using namespace std;

    void armstrong(int num)
    {
        int n,r,sum=0,temp;
        cout<<"enter the number"<<endl;
        cin>>n;
        temp=n;
        while(n>0)
        {
            r=n%10;
            sum=sum+(r*r*r);
            n=n/10;
        }
        if(temp==sum)
        cout<<"number is armstrong "<<endl;
        else
                cout<<"number is  not armstrong "<<endl;

    }


int main()
{
    int n;
  //  cout<<"enter the number"<<endl;
  // cin>>n;
    armstrong(n);
}
