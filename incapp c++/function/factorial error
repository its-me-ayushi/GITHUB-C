#include <iostream>

using namespace std;

    void factorial(int num)
    {
     int n,fact,number;
     cout<<"enter the positive number"<<endl;
     cin>>n;
     for(int i=0;i<=number;++i){
         fact=fact*i;
     }
     cout<<"factorial of"<<n<< " = "<<factorial<<endl;
     
}
int main()
{
    int n;
  //  cout<<"enter the number"<<endl;
  // cin>>n;
    factorial(n);
}
