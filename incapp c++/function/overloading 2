#include <iostream>

using namespace std;
void add(int a,int b)

{
   int result=a+b;
    cout<<"additon of integer: "<<result<<endl;
    
}
void add(int a,float b)
{
   float result=a+b;
    cout<<"additon of one int + one float: "<<result<<endl;
    
    
}
void add(double a,char b,int c)
{
   char result=a+b+c;
    cout<<"additon of one double+ one char +one int: "<<result<<endl;
    
}

int main()
{
    add(10,20);
    add(13,23.4f);
    add(14.3,'A',34);
}
