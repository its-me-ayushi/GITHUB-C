#include <iostream>

using namespace std;
void addtwo(int a,int b)
{
    int result=a+b;
    cout<<"addition of interger:"<<result<<endl;
    
}
void addthree(int a,int b,int c)
{
    int result=a+b+c;
    cout<<"addition of interger:"<<result<<endl;
    
}
void addfour(int a,int b,int c=1,int d=2)
{
    int result=a+b+c+d;
    cout<<"addition of interger:"<<result<<endl;
    
}



int main()
{
   addtwo(10,23);
  // addthree(13,12,34);
   addfour(13,45,56,23);
 //  addtwo(12,43);
}
