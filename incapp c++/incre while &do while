#include<iostream>
using namespace std;
int main()
{
    int a= 10,b;
     a=a++;
     cout<<"a="<<a<<endl;
     a=a++;
     cout<<"a="<<a<<endl;
     b= ++a;
     cout<<"b="<<b<<endl;
}
