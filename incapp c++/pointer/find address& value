{
int a=10;
cout<<"value of a:"<<a<<endl;
cout<<"address of a:"<<&a<<endl;
cout<<"value of a:"<<*&a<<endl;
float f=10;
cout<<"value of f:"<<f<<endl;
cout<<"address of f:"<<&f<<endl;
cout<<"value of f:"<<*&f<<endl;
char c='A';
cout<<"value of c:"<<c<<endl;
cout<<"address of c:"<<(void*)&c<<endl;
cout<<"address of c:"<<(int*)&c<<endl;
cout<<"value of c:"<<*&c<<endl;

}
