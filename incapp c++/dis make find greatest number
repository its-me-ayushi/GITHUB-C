#include(iostream)
using namespace std;
int main()
{
    int a,b,c;
    cout<<"enter the greatest number:"<<endl;
    cin>>a>>b>>c;
    if(a>b&&a>c)
    {
     cout<<"a is greatest"<<a<<endl;
    }
    if(b>a&&b>c)
     {
         cout<<"b is greatest"<<b<<endl;
         
     }
     if(c>a&&c>b)
     {
        cout<<"c is greatest"<<c<<endl;
     }
     else
     cout<<"invlid input"<<endl;        
     return 0;
}
