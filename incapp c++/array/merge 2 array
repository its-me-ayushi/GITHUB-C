#include<iosteam>
using namespace std;
int main()
{
        int a[10], b[10], c[20];
	int s1, s2, i, j;
    cout<<"Enter the Size for First Array: ";
    cin>>s1;
    cout<<"Enter "<<s1<<" Elements for First Array: ";
    for(i=0; i<s1; i++)
    {
        cin>>a[i];
        c[i] = a[i];
    }
    j = i;
    cout<<"\nEnter the Size for Second Array: ";
    cin>>s2;
    cout<<"Enter "<<s2<<" Elements for Second Array: ";
    for(i=0; i<s2; i++)
    {
        cin>>b[i];
        c[j] = b[i];
        j++;
    }
    cout<<"the newly array"<<endl;
    for(i=0; i<j; i++)
        cout<<c[i]<<" ";
    cout<<endl;
    return 0;
