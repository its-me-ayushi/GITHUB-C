#include <iostream>

using namespace std;


    void result(int b[5])
    {
        int t=0;
        float p;
        
      cout<<"enter five subject"<<endl;
      for(int i=0;i<5;i++)
    {
              t=t+b[i];
    }          
      cout<<"total marks:"<<t<<endl;
      p=t/5.0;
      cout<<"percentage"<<p<<endl;
      
     
      
    }
    int main()
    {
        int m[5],total;
        float per;
        cout<<"enter five subject"<<endl;
        for(int i=0;i<5;i++)
        {
            cout<<"subject"<<i+1<<" ";
            cin>>m[i];
        }
        result(m);
    }
