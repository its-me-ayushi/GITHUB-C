#include<stdio.h>
int main()
{
   unsigned char ch='A';
    int x,y;
    printf("enter row:\n");
    scanf("%d",&x);
    printf("enter column:\n");
    scanf("%d",&y);
    
    
    for(int i=1; i<=x; i++)
    {
        for(int j=1; j<=y; j++)
        {
        printf(" %c ascii=%d\t ",ch,ch);
        ch++;
        
        }
        
    printf(" \n ");
        }
    return 0;
}
