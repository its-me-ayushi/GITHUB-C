#include<stdio.h>
#include<stdlib.h>

struct node{
       int data;
       struct Node*next;
};
void LinkedListTraversal(struct Node*ptr)
{
    while(ptr!=NULL)
    {
        printf("element: %d\n",ptr->data);
        ptr = ptr->next;
    }
}
 int main()
 {
    struct Node*head;
    struct Node*second;
    struct Node*third;

    head=(struct Node*)malloc(size of(struct Node));
    second=(struct Node*)malloc(size of(struct Node));
    third=(struct Node*)malloc(size of(struct Node));

head->data=7;
head->next=second;

second->data=11;
second->next=third;

third->data=66;
third->next=NULL;

LinkedListTraversal(head);
return 0;
 }
